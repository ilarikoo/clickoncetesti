<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="ThemeDisplayName">Pinkki harmonia</system:String>

    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryColor" Color="#ffa1ca"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#a37cb1"/>
    <SolidColorBrush x:Key="BackgroundColor" Color="#eedde7"/>

    <SolidColorBrush x:Key="ButtonColor_Hover" Color="#ffffff"/>
    <SolidColorBrush x:Key="ButtonColor_Pressed" Color="#4c2d57"/>

    <SolidColorBrush x:Key="FontColor_Primary" Color="#000000"/>

    <SolidColorBrush x:Key="TitleBar_ButtonColor" Color="#000000"/>

</ResourceDictionary>